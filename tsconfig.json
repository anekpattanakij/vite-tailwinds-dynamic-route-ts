{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "rootDir": "./",
    "baseUrl": "./",
    "outDir": "./dist/ts",
    "paths": {
      "$src/*": [
        "./src/*"
      ],
      "$route/*": [
        "./src/routes/*"
      ],
      "$comp/*": [
        "./src/components/*"
      ],
      "$context/*": [
        "./src/contexts/*"
      ],
      "$layout/*": [
        "./src/layouts/*"
      ],
      "$store": [
        "./src/stores/index"
      ],
      "$store/*": [
        "./src/stores/*"
      ],
      "$util/*": [
        "./src/utils/*"
      ],
      "$type/*": [
        "./src/types/*"
      ],
      "$style/*": [
        "./src/styles/*"
      ],
      "$asset/*": [
        "./src/assets/*"
      ],
      "$/*": [
        "./*"
      ]
    }
  },
  "include": [
    "src"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}